generator client {
  provider = "prisma-client"
  output   = "../repositories/prisma/generated"
}

datasource db {
  provider = "postgresql"
}

model AccountType {
  identifier    String  @id @default(uuid())
  name          String  @unique
  rate          Float
  limitByClient Int?
  description   String?

  @@map("account_types")
}
